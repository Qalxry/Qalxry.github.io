/* abril-fatface-regular - latin */
@font-face {
    font-display: swap;
    /* Check https://developer.mozilla.org/en-US/docs/Web/CSS/@font-face/font-display for other options. */
    font-family: 'SmileySans';
    /* font-style: normal; */
    /* font-weight: 400; */
    src: url('../fonts/SmileySans/SmileySans-Oblique.ttf.woff2') format('woff2'),
        /* Chrome 36+, Opera 23+, Firefox 39+ */
        url('../fonts/SmileySans/SmileySans-Oblique.otf') format('opentype'),
        url('../fonts/SmileySans/SmileySans-Oblique.ttf') format('truetype');
}
